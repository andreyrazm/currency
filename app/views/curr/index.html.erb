<div class="section"><br><br><br>
    <div class="col-md-4"></div>

    <div class="col-md-2">
      <p id="a"><%= @m%></p>
        <table class="table table-striped">
          <tr><th>Валюта</th><th>Курс</th></tr>
          <% @cur.each_with_index do |p, i| %>
              <tr>
                <td id="val<%=i %>"><%= @val[i] %></td>
                <td id="cur<%=i %>"><%=p %> руб.</td>
              </tr>
          <%end%>
        </table>
    </div>
    <div class="col-md-1">
      <input type="text" class="datepicker" id="d">
    </div>
</div>